{
  "$schema": "https://unpkg.com/knip@latest/schema.json",
  "entry": ["src/index.tsx"],
  "project": ["src/**/*.{ts,tsx}"],
  "ignore": [
    "dist/**",
    ".rsbuild/**",
    "coverage/**",
    "src/**/routeTree.gen.ts"
