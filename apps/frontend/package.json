{
  "name": "@favoritable/frontend",
  "description": "Frontend application for the favoritable project",
  "version": "0.0.1",
  "author": "Thiago Mendonca",
  "private": true,
  "type": "module",
  "homepage": "https://github.com/trystan2k/favoritable#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/trystan2k/favoritable"
  },
  "bugs": {
    "url": "https://github.com/trystan2k/favoritable/issues"
  },
  "keywords": [
    "hono",
    "bookmarks",
    "favoritable",
    "frontend",
    "React",
    "TypeScript",
    "Rsbuild"
  ],
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "rsbuild dev --open",
    "build": "rsbuild build",
    "typecheck": "tsc --noEmit",
    "lint": "biome lint --error-on-warnings",
    "lint:fix": "pnpm lint --write",
    "lint:fix:unsafe": "pnpm lint --write --unsafe",
    "format": "biome format",
    "format:fix": "pnpm format --write",
    "check": "biome check --formatter-enabled=false --linter-enabled=false",
    "check:fix": "pnpm check --write",
    "knip": "knip",
    "preview": "rsbuild preview"
  },
  "license": "MIT",
  "dependencies": {
    "react": "~19.1.0",
    "react-dom": "~19.1.0"
  },
  "devDependencies": {
    "@favoritable/config-biome": "workspace:*",
    "@favoritable/typescript-config": "workspace:*",
    "@rsbuild/core": "~1.4.3",
    "@rsbuild/plugin-react": "~1.3.3",
    "@types/react": "~19.1.8",
    "@types/react-dom": "~19.1.6",
    "knip": "~5.62.0",
    "typescript": "~5.8.3"
  }
}
